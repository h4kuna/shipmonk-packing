{
    "name": "shipmonk/packing",
    "description": "Testing day application stub",
    "require": {
        "php": ">=8.4",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/orm": "3.*",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "2.*",
        "nette/bootstrap": "^3.2",
        "nette/caching": "^3.3",
        "nette/schema": "^1.3",
        "nette/utils": "^4.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0",
        "strictphp/http-clients": "dev-fixies",
        "symfony/cache": "7.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^12.0",
        "tracy/tracy": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "stan": "vendor/bin/phpstan analyse"
    }
}
